<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android = "http://schemas.android.com/apk/res/android"
    package = "com.example.android.pharmtechapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="pharmatech.MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="pharmatech.ReviewActivity"
                  android:label="Review">
        </activity>

        <activity android:name="pharmatech.MultipleChoicesQuiz"
                  android:label="MultiChoice Quiz">
        </activity>

        <activity android:name="pharmatech.PracticeMenuActivity"
                  android:label="">
        </activity>

        <activity android:name="pharmatech.FlashcardActivity"
                  android:label="Flashcard Mode">
        </activity>

    </application>

</manifest>